@article{robinson1977,
	abstract = {In 3 experiments with 98 5-, 6-, 7-, and 9-yr-olds, each child and the experimenter played a game in which one player, the speaker, described one of a set of drawings so that the other, the listener, could pick the matching one from an identical set. When communication failed, the child was asked to ascribe blame to the listener and/or the speaker. No relation was found between ascriptions of blame and inadequacy of the misunderstood message in either Exp I or II. In Exp I, the inadequacy measure was the number of drawings in the set to which a message referred; in Exp II, the inadequacy measure was the percentage of a sample of adults who understood a message. In the 3rd experiment, when a message referred to more than one card, some children were more likely to judge it inadequate when the listener chose the wrong card than when he chose the right one. The frequency of these outcome-centered judgments decreased with age. Results are discussed in terms of developmental changes in the},
	author = {Robinson, E. J. and Robinson, W. P.},
	issn = {0012-1649},
	journal = {Developmental Psychology},
	keywords = {communication failure, ascription of blame & judgments of message adequacy, 5 vs 6 vs 7 vs 9 yr olds, Age Differences, Attribution, Failure, Messages, Verbal Communication, Cognitive Development},
	number = {2},
	pages = {156 - 161},
	title = {Children's explanations of communication failure and the inadequacy of the misunderstood message.},
	volume = {13},
	year = {1977},
	bdsk-url-1 = {http://proxy.uchicago.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=1977-27228-001&site=ehost-live&scope=site}}

@article{snow1972,
	abstract = {The assumption that language acquisition is relatively independent of the amount and kind of language input must be assessed in light of information about the speech actually heard by young children. The speech of middle-class mothers to 2-year-old children was found to be simpler and more redundant than their speech to 10-year-old children. The mothers modified their speech less when talking to children whose responses they could not observe, indicating that the children played some role in eliciting the speech modifications. Task difficulty did not contribute to the mothers' production of simplified, redundant speech. Experienced mothers were only slightly better than nonmothers in predicting the speech-style modifications required by young children. These findings indicate that children who are learning language have available a sample of speech which is simpler, more redundant, and less confusing than normal adult speech.},
	author = {Snow, Catherine E},
	date-modified = {2019-01-28 12:39:24 -0600},
	file = {:C$\backslash$:/Users/Ashley/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Snow - 1972 - Mothers' Speech to Children Learning Language.pdf:pdf},
	journal = {Child Development},
	number = {2},
	pages = {549--565},
	title = {{Mothers' Speech to Children Learning Language}},
	volume = {43},
	year = {1972}}
