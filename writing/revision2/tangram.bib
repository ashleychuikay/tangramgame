	issn = {1469-7602, 0305-0009},
	journal = {Journal of Child Language},
	language = {en},
	month = jun,
	number = {2},
	pages = {439--463},
	shorttitle = {Interactive communication between children},
	title = {Interactive communication between children: learning how to make language work in dialogue},
	urldate = {2020-01-15},
